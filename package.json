{
  "//1": "describes your app and its dependencies",
  "//2": "https://docs.npmjs.com/files/package.json",
  "//3": "updating this file will download and update your packages",
  "name": "dynamo-easy-glitch",
  "version": "0.0.1",
  "description": "simple project as showcase for dynamo-easy on node",
  "license": "MIT",
  "keywords": [
    "node",
    "glitch",
    "@shiftcoders/dynamo-easy",
    "aws",
    "dynamo",
    "shiftcode"
  ],
  "main": "./dist/index.js",
  "engines": {
    "node": "8.x"
  },
  "repository": {
    "url": "https://glitch.com/#!/project/welcome-project"
  },
  "scripts": {
    "lint": "tslint -t codeFrame '{src, test}/**/*.ts' --project ./tsconfig.json --fix",
    "build": "tsc",
    "start": "ts-node ./src/index.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "{src,test}/**/*.ts": [
        "npx prettier --write --config ./.prettierrc.yml",
        "npx tslint --fix --project tsconfig.json",
        "git add"
      ]
    }
  },
  "dependencies": {
    "@shiftcoders/dynamo-easy": "^4.1.0-beta.17",
    "@types/moment-timezone": "^0.5.8",
    "@types/node": "^8.10.37",
    "@types/request-promise-native": "^1.0.15",
    "aws-sdk": "^2.385.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "moment-timezone": "^0.5.23",
    "reflect-metadata": "^0.1.12",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5",
    "rxjs": "^6.3.3",
    "ts-node": "^7.0.1",
    "tslib": "^1.9.3",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.2.2",
    "uuid": "^3.3.2"
  }
}
