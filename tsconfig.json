{
  "compilerOptions": {
    "moduleResolution": "node",
    "target": "es2016",
    "module": "commonjs",
    "lib": [
      "es2016",
      "esnext.asynciterable",
      "es2017.object",
      "es2017.string"
    ],
    "strict": true,
    "resolveJsonModule": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "importHelpers": true,
    "allowJs": true,

    "sourceMap":  true,
    "outDir": "./dist/"
  }
}